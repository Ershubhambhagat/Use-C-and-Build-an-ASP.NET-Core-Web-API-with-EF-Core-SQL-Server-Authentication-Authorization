@model IEnumerable<NZWalk.UI.Models.DTOs.RegionDto>

<style >.custom-table {
    border: 3px solid green;
    border-collapse: collapse;
}
.custom-table th, .custom-table td {
    padding: 10px;
    text-align: left;
    border: 3px solid green;
}
</style>
<h1 class="mt-3">Regions</h1>
<table class="custom-table">
    <thead>
        <tr>
            <th>Id </th>
            <th>Code</th>
            <th>Name</th>

        </tr>
    </thead>
    <tbody>

        @foreach (var walk in Model)
        {
          <tr>
                <td>@walk.Id</td>
                <td>@walk.Name </td>
                <td>@walk.Code </td>
                <td>@walk.RegionImageUrl </td>


          </tr>  
        }

    </tbody>
</table>




